{
    "name": "login",
    "productName": "Login-Panel",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make --platform=win32 --arch=x64",
        "publish": "electron-forge publish",
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx --max-warnings 0",
        "prepare": "husky install",
        "build": "node webpack.cicd.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write",
            "yarn lint"
        ]
    },
    "keywords": [],
    "author": {
        "name": "Yuan Yang",
        "email": "yuanyang@tsinghua.edu.cn"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./icon.ico"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "login"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                {
                    "name": "@electron-forge/plugin-webpack",
                    "config": {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/renderer.tsx",
                                    "name": "main_window"
                                }
                            ]
                        },
                        "devServer": {
                            "liveReload": false,
                            "devMiddleware": {
                                "writeToDisk": false
                            }
                        },
                        "devContentSecurityPolicy": "style-src 'self' 'unsafe-inline'; connect-src * data: blob: 'unsafe-inline'; img-src * 'self' data: blob: 'unsafe-inline';",
                        "port": 3000,
                        "loggerPort": 9000
                    }
                }
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0",
        "@electron-forge/maker-deb": "^6.0.0",
        "@electron-forge/maker-rpm": "^6.0.0",
        "@electron-forge/maker-squirrel": "^6.0.0",
        "@electron-forge/maker-zip": "^6.0.0",
        "@electron-forge/plugin-webpack": "^6.0.0",
        "@material-ui/core": "^4.12.2",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "@reduxjs/toolkit": "^1.6.1",
        "@types/fs-extra": "^11.0.4",
        "@types/qrcode.react": "^1.0.1",
        "@types/react": "^18.0.5",
        "@types/react-dom": "^17.0.9",
        "@types/react-router": "^5.1.16",
        "@types/react-router-dom": "^5.1.6",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^4.0.1",
        "@vercel/webpack-asset-relocator-loader": "1.6.0",
        "axios": "^1.7.2",
        "css-loader": "^6.0.0",
        "electron": "13.1.7",
        "electron-squirrel-startup": "^1.0.0",
        "eslint": "^7.6.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.20.0",
        "fork-ts-checker-webpack-plugin": "^6.0.1",
        "husky": "^7.0.0",
        "lint-staged": "^11.1.1",
        "node-loader": "^2.0.0",
        "qrcode.react": "^1.0.1",
        "react-refresh": "^0.14.0",
        "react-refresh-typescript": "^2.0.9",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-star-ratings": "^2.3.0",
        "style-loader": "^3.0.0",
        "ts-loader": "^9.2.2",
        "typescript": "^4.0.2",
        "xlsx": "^0.17.0"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^5.0.5",
        "@mui/lab": "^5.0.0-alpha.53",
        "@mui/material": "^5.2.1",
        "@mui/styles": "^5.2.3",
        "@mui/x-data-grid": "^5.0.1",
        "@react-pdf/renderer": "^3.0.0",
        "@types/crypto-js": "^4.1.0",
        "@types/lodash": "^4.14.202",
        "@types/pako": "^2.0.3",
        "@types/qrcode": "^1.5.5",
        "antd": "^5.11.5",
        "class-transformer": "^0.5.1",
        "compressing": "^1.10.0",
        "copy-webpack-plugin": "^11.0.0",
        "crypto-js": "^4.1.1",
        "date-fns": "^3.6.0",
        "element-react": "^1.4.34",
        "element-theme-default": "^1.4.13",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^2.4.0",
        "file-loader": "^6.2.0",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.2.0",
        "idb-keyval": "^6.2.1",
        "js-base64": "^3.7.5",
        "jsonfile": "^6.1.0",
        "lodash": "^4.17.21",
        "madge": "^7.0.0",
        "minio": "^7.1.3",
        "moment": "^2.30.1",
        "pako": "^2.1.0",
        "path-browserify": "^1.0.1",
        "pinyin": "^4.0.0-alpha.0",
        "prettier": "^2.3.2",
        "qrcode": "^1.5.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-draggable": "^4.4.6",
        "redux-persist": "^6.0.0",
        "reflect-metadata": "^0.1.13",
        "slash": "^5.1.0",
        "terser-webpack-plugin": "^5.1.4",
        "zustand": "^3.5.13"
    }
}
